<navigator url="../personform/personform" class="msgbox">
  <view class="msgbox_in">
    <image class="msgbox_in_img" src="{{userInfo.avatarUrl}}../../image/match1.jpg" />
    <view class="msgbox_in_text_title">{{userInfo.nickName}}</view>
    <view class="msgbox_in_text_title">卢宇博{{man.studentName}}</view>
    <view class="msgbox_in_text_desc">软件工程{{man.major}}</view>
    <view class="msgbox_in_text_desc">12313216546{{man.contacts}}</view>
  </view>
</navigator>




<view class="list">
  <navigator url="../teamlist_create/teamlist_create" class="list_item" hover-class="weui-cell_active">
    <image class="list_item_img" src="{{userInfo.avatarUrl}}../../image/灯泡.png" />
    <view class="list_item_text">我创建的队伍</view>
<view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>



  <navigator url="../teamlist_join/teamlist_join" class="list_item" hover-class="weui-cell_active">
    <image class="list_item_img" src="{{userInfo.avatarUrl}}../../image/握手.png" />
    <view class="list_item_text">我加入的队伍</view>
<view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>


  <navigator url="../teamlist_collect/teamlist_collect" class="list_item" hover-class="weui-cell_active">
    <image class="list_item_img" src="{{userInfo.avatarUrl}}../../image/星星.png" />
    <view class="list_item_text">我收藏的队伍</view>
<view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>

