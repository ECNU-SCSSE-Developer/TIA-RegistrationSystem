<view class='pages'>
  <view class="weui-panel">
    <view class="weui-panel__hd">
      <!--navigator wx:for="{{team.data}}" wx:key="{{team.data}}" url="../../team/team?objectId={{item.objectId}}" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active" id="{{item.id}}"-->
     
      <navigator wx:for="{{team.data}}" wx:key="{{team.data}}" bindtap="openConfirm" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active" wx:for-index="idx" data-name="{{idx}}">
